---
pageable: true
per_page: 10
---
<div class="blog container container-vertical-top">
  <h1>Articles by tag "<%= tagname %>"</h1>
  <% page_articles.each_with_index do |article| %>
    <%= partial "partials/article_preview", locals: {article: article} %>
  <% end %>
  <button id="paginate-control" class="btn btn--primary paginate-control is-paginate-control-hidden">Load more</button>
</div>
