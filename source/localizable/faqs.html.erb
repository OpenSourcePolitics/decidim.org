<div class="container container-vertical">
  <h1 class="page-title">Frequently asked questions</h1>
  <% @faqs = data.faqs %>
  <div class="partner__container container--text-col">
    <% @faqs.each do |id, question| %>
      <dl class="faqs">
        <dt><%= question["title"] %></dt>
        <dd><%= markdown(question["answer_en"]) %></dd>
      </dl>
    <% end %>
  </div>
</div>